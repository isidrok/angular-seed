{
  "name": "mv",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "client/*",
    "infra/*"
  ],
  "scripts": {
    "start": "webpack-dev-server --config infra/builder/webpack.config.dev.js",
    "build": "webpack --config infra/builder/webpack.config.prod.js",
    "test": "karma start infra/ut/karma.conf.js",
    "test:once": "karma start infra/ut/karma.conf.js --single-run",
    "test:e2e": "codeceptjs run -c infra/e2e/codecept.conf.js",
    "serve": "http-server dist --proxy http://localhost:8080/? -b -c31536000"
  },
  "dependencies": {
    "@angular/animations": "8.2.7",
    "@angular/common": "8.2.7",
    "@angular/compiler": "8.2.7",
    "@angular/core": "8.2.7",
    "@angular/forms": "8.2.7",
    "@angular/platform-browser": "8.2.7",
    "@angular/router": "8.2.7",
    "core-js": "3.2.1",
    "rxjs": "6.5.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.4.0",
    "@types/node": "12.7.5",
    "http-server": "0.11.1",
    "npm-run-all": "4.1.5",
    "typescript": "3.5"
  }
}
